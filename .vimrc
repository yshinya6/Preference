:syntax enable
set number
set laststatus=2
set statusline=%F%l%=
set incsearch
set wildmenu wildmode=list:full
set mouse=a
set expandtab
set tabstop=4
set shiftwidth=4
set softtabstop=4
set autoindent
set smartindent
set clipboard=unnamed,autoselect
language C
inoremap { {}<Left>
inoremap [ []<Left>
inoremap ( ()<Left>
inoremap " ""<Left>
inoremap ' ''<Left>
inoremap < <><Left>
set backspace=indent,eol,start
